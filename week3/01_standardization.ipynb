      ],
      "source": [
        "import numpy as np\n",
        "\n",
        "data_2d = np.array([[1, 10],\n",
        "                    [2, 20],\n",
        "                    [3, 30]])\n",
        "m = np.mean(data_2d, axis=0)\n",
        "s = np.std(data_2d, axis=0)\n",
        "\n",
        "standardized_data_2d = (data_2d-m)/s\n",
        "\n",
        "print(\"元の2次元データ:\\n\", data_2d)\n",
        "print(\"標準化された2次元データ（列ごと）:\\n\", standardized_data_2d)"
      ]
    }
  ]
}